<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="app.css" />
    <link rel="stylesheet" href="BlazorAuthentication.styles.css" />
    <link rel="icon" type="image/png" href="favicon.png" />
    <link rel="stylesheet" type="text/css" href="css/styles.css" />
    <link rel="stylesheet" href="_content/Radzen.Blazor/css/standard-base.css">
    <link rel="stylesheet" href="bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link href="_content/Syncfusion.Blazor/styles/bootstrap5.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" />
    <HeadOutlet />
</head>

<body>
    <div id="app" style="background-color: white;">
        <Routes  />
    </div>
    <script src="_content/MudBlazor/MudBlazor.min.js"></script>
    <script src="_content/Radzen.Blazor/Radzen.Blazor.js?v=@(typeof(Radzen.Colors).Assembly.GetName().Version)"></script>
    <script src="_content/Syncfusion.Blazor/scripts/syncfusion-blazor.min.js" type="text/javascript"></script>
    <script src="disposeResources.js"></script>
    <script>
        window.registerDotNetObject = function (dotNetObject) {
            window.dotNetObject = dotNetObject;
        };

        window.unregisterDotNetObject = function (dotNetObject) {
            if (window.dotNetObject === dotNetObject) {
                window.dotNetObject = null;
            }
        };

        window.someJsFunction = function () {
            if (window.dotNetObject) {
                window.dotNetObject.invokeMethodAsync('SomeMethod').catch(error => {
                    console.error("DotNetObjectReference has been disposed.");
                });
            }
        };

        window.addEventListener('beforeunload', function () {
            if (window.dotNetObject) {
                window.dotNetObject.invokeMethodAsync('Dispose');
            }
        });

    </script>
    <script src="_framework/blazor.web.js"></script>
</body>

</html>
